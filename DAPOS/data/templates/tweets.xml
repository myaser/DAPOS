{% load dapos_tags %}<?xml version="1.0" encoding="utf-8"?>
<django-objects version="1.0">
{% for index,list in lists %}
    <object pk="{{index}}" model="data.tweets">
        <field type="CharField" name="user_id">{{list.0}}</field>
        <field type="CharField" name="username">{{list.1}}</field>
        <field type="CharField" name="influence">{{list.2}}</field>
        <field type="TextField" name="tweet_text">{{list.3}}</field>
        <field type="CharField" name="tweet_id">{{list.4}}</field>
        <field type="DateField" name="posting_date">{{list.5}}</field>
        <field type="IntegerField" name="retweets">{{list.6}}</field>
        <field type="TextField" name="hash_tags">{{list.7|encode_list}}</field>
        <field type="TextField" name="mentions">{{list.8|encode_list}}</field>
        <field type="TextField" name="links">{{list.9|encode_list}}</field>
    </object>
{% endfor %}
</django-objects>
