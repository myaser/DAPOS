<?xml version="1.0" encoding="utf-8"?>
<django-objects version="1.0">
{% for index,list in lists %}
    <object pk="{{index}}" model="data.alkhalilsuffix">
        <field type="CharField" name="unvoweled">{{list.0}}</field>
        <field type="CharField" name="voweled">{{list.1}}</field>
        <field type="TextField" name="description">{{list.2}}</field>
        <field type="CharField" name="type">{{list.3}}</field>
        <field type="TextField" name="pattern">No_Pattern</field>
    </object>
{% endfor %}
</django-objects>
